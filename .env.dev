# Development environment configuration
# Loaded automatically by justfile (set dotenv-path := ".env.dev")

# PostgreSQL
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin123
POSTGRES_DB=sub2api
DATABASE_PORT=5432

# Redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Admin account
ADMIN_EMAIL=admin@123.com
ADMIN_PASSWORD=admin123

# Server
SERVER_MODE=debug
SERVER_PORT=8080
EMBED_PORT=8081

# Go module proxy (avoids proxy.golang.org which is blocked in China)
GOPROXY=https://goproxy.cn,direct

# HTTP proxy for outbound requests (e.g. dev-be, pnpm, scoop)
HTTP_PROXY=http://127.0.0.1:7897
HTTPS_PROXY=http://127.0.0.1:7897
